type Query {
    getStudentByName(name: String): Student
    getAllStudents: [Student]
    getAllDegrees: [Degree]
}

type Student {
    name: String
    surname: String
    degree: Degree
    tutor: Tutor
}

type Degree {
    degree: String
}

type Tutor {
    name: String
    surname: String
    degree: Degree
}